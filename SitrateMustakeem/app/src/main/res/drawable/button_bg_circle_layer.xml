<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
<item>
    <shape
        android:shape="oval">
          <solid android:color="@color/light_blue"/>
        <stroke android:color="@color/light_grey" android:width="1dp"/>

        <padding android:top="@dimen/_2sdp" android:bottom="@dimen/_2sdp"
            android:right="@dimen/_2sdp" android:left="@dimen/_2sdp"/>
        <size android:width="@dimen/_45sdp" android:height="@dimen/_45sdp"/>
    </shape>
</item>

<item>
    <shape
        android:shape="oval">
        <solid android:color="@color/white"/>
        <padding android:top="@dimen/_4sdp" android:bottom="@dimen/_4sdp"
            android:right="@dimen/_4sdp" android:left="@dimen/_4sdp"/>
        <size android:width="@dimen/_40sdp" android:height="@dimen/_40sdp"/>
    </shape>
</item>

</layer-list>
